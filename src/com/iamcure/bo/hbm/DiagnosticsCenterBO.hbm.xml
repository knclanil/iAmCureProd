<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="om.iamcure.bo.UserBO" table="diagnosticcenter">
    <id name="U_id" column="U_id">
      <generator class="identity" />
    </id>
	<property name="DiagnosticCenterName" column="DiagnosticCenterName" not-null="true" />
	<property name="Country" column="Country" not-null="true" />
	<property name="StateName" column="StateName" not-null="true" />
	<property name="City" column="City" not-null="true" />
	<property name="PinCode int" column="PinCode int" not-null="true" />
	<property name="StreetName" column="StreetName" not-null="true" />
	<property name="PhoneNumber" column="PhoneNumber" not-null="true" />
	<property name="DiagnosticCenterPhoto" column="DiagnosticCenterPhoto" not-null="true" />
	<property name="Descripition" column="Descripition" not-null="true" />
	<property name="CreatedDate" column="CreatedDate datetime" not-null="true" />
	<property name="LastModifiedDate" column="LastModifiedDate datetime" not-null="true" />
	<property name="CreatedBy" column="CreatedBy" not-null="true" />
	<property name="LastModifiedBy" column="LastModifiedBy" not-null="true" />
	<property name="ContactPerson" column="ContactPerson" not-null="true" />
	<property name="SamplesPickup" column="SamplesPickup" not-null="true" />
	<property name="DiagnosticsCenterLicenseNumber" column="DiagnosticsCenterLicenseNumber" not-null="true" />
	<property name="DiagnosticsCenterLicenseDocument" column="DiagnosticsCenterLicenseDocument" not-null="true" />
	<property name="IsVerified" column="IsVerified" not-null="true" />
  </class>
</hibernate-mapping>
