<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="om.iamcure.bo.MedicalStoreBO" table="medicalstore">
    <id name="U_id" column="U_id">
      <generator class="identity" />
    </id>
    <property name="fullName" column="fullName" not-null="true" />
	<property name="MedicalstoreName" column="MedicalstoreName" not-null="true" />
	<property name="Country" column="Country" not-null="true" />
	<property name="StateName" column="StateName" not-null="true" />
	<property name="City" column="City" not-null="true" />
	<property name="PinCode" column="PinCode" not-null="true" />
	<property name="StreetName" column="StreetName" not-null="true" />
	<property name="PhoneNumber" column="PhoneNumber" not-null="true" />
	<property name="DiagnosticCenterPhoto" column="DiagnosticCenterPhoto" not-null="true" />
	<property name="Descripition" column="Descripition" not-null="true" />
	<property name="StoreTimings" column="StoreTimings" not-null="true" />
	<property name="StoreRating" column="StoreRating" not-null="true" />
	<property name="StoreReview" column="StoreReview" not-null="true" />
	<property name="MedicalPrescription" column="MedicalPrescription" not-null="true" />
	<property name="Report" column="Report" not-null="true" />
	<property name="CreatedDate" column="CreatedDate" not-null="true" />
	<property name="LastModifiedDate" column="LastModifiedDate" not-null="true" />
  </class>
</hibernate-mapping>
