<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.iamcure.bo.UserBO" table="user">
    <id name="id" column="id">
      <generator class="identity" />
    </id>
	<property name="phoneNumber" column="phoneNumber" not-null="true" />
	<property name="email" column="email" />
	<property name="password" column="password" not-null="true" />
	<property name="type" column="type" not-null="true" />
	<property name="fullName" column="fullName" not-null="true" />
	<!--<property name="createdDate" column="createdDate" not-null="true" />-->
	</class>
</hibernate-mapping>
