<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="om.iamcure.bo.UserPaymentHistBO" table="UserPaymentHistBO">
	<id name="id" column="U_ID">
		  <generator class="identity" />
		</id>
	<property name="User_ID" column="User_ID" not-null="true"/>
	<property name="Payment_Date" column="Payment_Date" not-null="true"/>
	<property name="Payment_Type" column="Payment_Type" not-null="true"/>
	<property name="CreatedDate" column="CreatedDate" not-null="true"/>
	<property name="Payment_Cancel" column="Payment_Cancel" not-null="true"/>
	<property name="ModifiedDate" column="ModifiedDate" not-null="true"/>
	</class>
</hibernate-mapping>