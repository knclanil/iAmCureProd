<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="om.iamcure.bo.UserBO" table="UserRandomBO">
	<id name="id" column="U_ID">
		  <generator class="identity" />
		</id>
	<property name="User_Name" column="User_Name" not-null="true"/>
	<property name="Singup_BloodGroup" column="Singup_BloodGroup" not-null="true"/>
	<property name="Signup_OrganDonation" column="Signup_OrganDonation" not-null="true"/>
	<property name="Frequent_Medicine_Order" column="Frequent_Medicine_Order" not-null="true"/>
	<property name="Fav_Doctor" column="Fav_Doctor" not-null="true"/>
	<property name="Fav_MedicalStore" column="Fav_MedicalStore" not-null="true"/>
	<property name="Fav_Lab" column="Fav_Lab" not-null="true"/>
	<property name="CreatedDate" column="CreatedDate" not-null="true"/>
	<property name="ModifiedDate" column="ModifiedDate" not-null="true"/>
	</class>
</hibernate-mapping>